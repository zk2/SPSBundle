<table id="sps_table" class="table table-striped table-condensed table-hover table-bordered">
    <thead>
    <tr>
        {% for column in paginator.columns %}
            {{ sps_pagination_sortable(column, paginator.usedRoute, paginator.usedRouteParams) }}
        {% endfor %}
    </tr>
    </thead>
    <tbody>
    {% for result in paginator %}
        <tr>
            {% for column in paginator.columns %}
                {{ sps_build_td(column, result) }}
            {% endfor %}
        </tr>
    {% endfor %}
    </tbody>
    <tfoot>
    {% if autosum is defined and autosum %}
        <tr class="zk-autosum">
            {{ sps_build_autosum(autosum, paginator.columns) }}
        </tr>
    {% endif %}
    {{ sps_pagination_pagination(paginator) }}
    </tfoot>
</table>
